    "db:seed:moroccan": "pnpm exec tsx src/db/moroccan-holidays-seed.ts",
    "db:seed:islamic": "pnpm exec tsx src/db/islamic-holidays-seed.ts",
    "db:studio": "drizzle-kit studio"
