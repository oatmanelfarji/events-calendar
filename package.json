{
  "name": "events-calendar",
  "private": "true",
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest run",
    "format": "pnpm exec biome format --write ./src",
    "lint": "pnpm exec biome lint ./src",
    "check": "pnpm exec biome check ./src",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:studio": "drizzle-kit studio",
    "db:seed": "pnpm db:seed:moroccan && pnpm db:seed:islamic && pnpm db:seed:season-start",
    "db:seed:moroccan": "pnpm exec tsx src/db/moroccan-holidays-seed.ts",
    "db:seed:islamic": "pnpm exec tsx src/db/islamic-holidays-seed.ts",
    "db:seed:season-start": "pnpm exec tsx src/db/season-start-seed.ts"
  },
