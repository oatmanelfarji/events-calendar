  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest run",
    "format": "pnpm exec biome format --write ./src",
    "lint": "pnpm exec biome lint ./src",
    "check": "pnpm exec biome check ./src",
    "storybook": "storybook dev -p 6006",
